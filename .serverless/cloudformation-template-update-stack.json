        ],
        "KeySchema": [
          {
            "AttributeName": "user_id",
            "KeyType": "HASH"
          },
          {
            "AttributeName": "task_id",
            "KeyType": "RANGE"
          }
        ],
        "LocalSecondaryIndexes": [
